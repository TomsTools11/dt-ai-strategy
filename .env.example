# DesignsTouch AI Services - Environment Variables
# Copy this file to .env and fill in the values

# App Configuration
VITE_APP_TITLE="DesignsTouch AI Services"
VITE_APP_LOGO="/DSLogo.svg"

# Analytics (Umami) - Optional
# Leave empty to disable analytics
VITE_ANALYTICS_ENDPOINT=""
VITE_ANALYTICS_WEBSITE_ID=""

# OAuth Configuration - Optional
# Only needed if using Manus OAuth
VITE_OAUTH_PORTAL_URL=""
VITE_APP_ID=""

# Map/Forge API - Optional
# Only needed if using the Map component with external API
VITE_FRONTEND_FORGE_API_KEY=""
VITE_FRONTEND_FORGE_API_URL=""

# Manus Runtime - Set to "true" only in Manus environment
# MANUS_RUNTIME="true"
